{
  "expo": {
    "name": "native-blank-type",
    "slug": "native-blank-type",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2,
      "useNextNotificationsApi": true,
      "sound": true,
      "priority": "max",
      "permissions": ["NOTIFICATIONS"],
      "package": "com.ama85bd.nativeblanktype"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/notify.wav"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3e629754-c328-47f4-8b5c-ce3ab592f100"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3e629754-c328-47f4-8b5c-ce3ab592f100"
    }
  }
}
